<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>ServletAPI</servlet-name>
        <servlet-class>com.google.api.server.spi.SystemServiceServlet</servlet-class>
        <init-param>
            <param-name>services</param-name>
            <param-value>org.myLazyClock.restApi.AlarmClockAPI,org.myLazyClock.restApi.CalendarAPI</param-value>
        </init-param>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServletAPI</servlet-name>
        <url-pattern>/_ah/spi/*</url-pattern>
    </servlet-mapping>

    <filter>
        <filter-name>ObjectifyFilter</filter-name>
        <filter-class>com.googlecode.objectify.ObjectifyFilter</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>ObjectifyFilter</filter-name>
        <url-pattern>/_ah/spi/*</url-pattern>
        </filter-mapping>

    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>

    <servlet>
        <servlet-name>ModulesServlet</servlet-name>
        <servlet-class>org.myLazyClock.restApi.ModulesServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>ModulesServlet</servlet-name>
        <url-pattern>/testModules</url-pattern>
    </servlet-mapping>
</web-app>
